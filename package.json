{
  "name": "Ponotes",
  "description": "in-browser note-taking application",
  "author": "Matthew Harvey <software@matthewharvey.net>",
  "private": true,
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/matt-harvey/ponotes"
  },
  "scripts": {
