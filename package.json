    "es6-promise": "^3.1.2",
    "es6-shim": "^0.35.0",
    "font-awesome": "^4.6.3",
    "lodash": "^4.15.0",
    "normalize.css": "^4.2.0",
    "pouchdb": "^6.0.5",
    "pouchdb-find": "^0.10.3",
    "primeng": "^1.0.0-beta.15",
